{
    "manifest_version": 2,
    "name": "ytsig",
    "version": "0.1",
    "description": "Prevents you from wasting your time and your brain on YouTube Shorts.",
    "permissions": ["https://www.youtube.com/shorts*"],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["ytsig.js"]
        }
    ]
}
